machine sdl.app:
	
	driver context:
		sdl.context
	
	chain program:
		
		state event.register
		
		loop center.events:
			center.customer
			sdl.event.pipe
			state.event.pipe:
				target = event.register
		
		loop video:
			center.customer
			center.video.prog.pipe
			sdl.prog.pipe:
				sizeable =		true
				env =			event.register
