int main:
	double a = 345    // an "integer", although there is only one numeric type in JavaScript
	double b = 34.5   // a floating-point number
	double c = 3.45e2 // another floating-point, equivalent to 345
	int d = 0377   // an octal integer equal to 255
	int e = 0xFF   // a hexadecimal integer equal to 255, digits represented by the letters A-F may be upper or lowercase
	
	return a==345 && b*10==345 && c==345 && d==255 && e==255
