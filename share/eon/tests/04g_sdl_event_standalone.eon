machine sdl.app:
		
	driver context:
		sdl.context
	
	chain program:
		state event.register
		
		loop event:
			center.customer
			sdl.event.pipe
			state.event.pipe:
				target = event.register
		
		loop video:
			center.customer
			center.video.src.dbg_generator:
				mode = "noise"
			sdl.video.pipe
