machine x11.app: {
	
	driver context: {
		x11.context: true;
	};
	
	chain program: {
		state event.register;
		
		loop video: {
			center.customer: true;
			x11.sw.fbo.program: true {
				drawmem:		"false";
				program:		"obj_view";
			};
			x11.sw.video.pipe: true {
				close_machine:	true;
				sizeable:		true;
				env:			event.register;
				program:		"obj_view";
			};
		};
		
	};

};
