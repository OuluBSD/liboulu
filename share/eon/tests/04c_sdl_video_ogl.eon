machine sdl.app:
	
	driver context:
		sdl.context
	
	chain program:
		
		loop ogl.fbo:
			ogl.customer
			sdl.fbo.standalone:
				shader.frag.path: "shaders/toys/simple/simple_single/stage0.glsl"
		
		loop audio:
			center.customer
			center.audio.src.dbg_generator:
				waveform: "noise"
			sdl.audio
