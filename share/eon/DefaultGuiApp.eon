machine sdl.app: {
		
	driver context: {
		sdl.context: true;
	};
		
	chain program: {
		
		state event.register;
		
		loop center.events: {
			center.customer: true;
			sdl.event.pipe: true;
			state.event.pipe: true {
                target: event.register;
            };
		};
		
		loop video: {
			center.customer: true;
			center.video.prog.ecs: true;
			sdl.prog.pipe: true {
				sizeable:		true;
				env:			event.register;
            };
		};
		
	};

};

