#include "IGraphics.h"


NAMESPACE_PARALLEL_BEGIN


template <class Gfx>
void InitObjView() {
	ShaderLibrary<Gfx>::template AddProgramClass<ObjViewProgT<Gfx>>("obj_view_program");
}

NAMESPACE_PARALLEL_END


INITBLOCK_(Shaders) {
	using namespace TS;
	using namespace TS::Parallel;
	
	#ifdef flagPOSIX
	InitObjView<X11SwGfx>();
	#ifdef flagOGL
	InitObjView<X11OglGfx>();
	#endif
	#endif
	
	#if defined flagSDL2 && defined flagOGL
	InitObjView<SdlOglGfx>();
	#endif
	
}

