
player.audio.generator: {
	reg.app.arg.mode: "generator";
	center.audio.test: true;
	center.audio.sink.hw: true;
}

player.audio.ffmpeg: {
	reg.app.arg.mode: "ffmpeg";
	perma.audio.source.file.path: reg.app.arg.file;
	center.audio.source.decoder.mp3: true;
	center.audio.sink.hw: true;
}

player.media.ffmpeg: {
	reg.appmode: "video";
	perma.video.source.file.path: reg.app.arg.file;
	center.video.source.decoder.mp4: true;
	center.video.sink.hw: true;
}
