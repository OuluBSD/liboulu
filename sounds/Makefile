sf2/05_epiano1/instrument.sf2:
	-@mkdir -p $(shell dirname $@)
	(cd $(shell dirname $@) && \
		wget -c http://download.linuxaudio.org/musical-instrument-libraries/sf2/jRhodes3.tar.7z && \
		7z x jRhodes3.tar.7z && \
		7z x jRhodes3.tar && \
		mv jRhodes3/jRhodes3.sf2 instrument.sf2 && \
		rm jRhodes3.tar)

