sf2/01_piano/instrument.sf2:
	-@mkdir -p $(shell dirname $@)
	(cd sf2/01_piano && \
		wget -c https://musical-artifacts.com/artifacts/2183/Casio_PX-860_Concert_Grand_Piano.zip && \
		7z x Casio_PX-860_Concert_Grand_Piano.zip && \
		mv "Casio PX-860 Concert Grand Piano.sf2" instrument.sf2)

sf2/02_piano/instrument.sf2:
	-@mkdir -p $(shell dirname $@)
	(cd sf2/02_piano && \
		wget -c https://musical-artifacts.com/artifacts/1314/VS_Upright_Piano_lite.sf2 && \
		mv VS_Upright_Piano_lite.sf2 instrument.sf2)

sf2/03_egrand/instrument.sf2:
	-@mkdir -p $(shell dirname $@)
	(cd $(shell dirname $@) && \
		wget -c https://musical-artifacts.com/artifacts/1812/House_Piano_4th.sf2 && \
		mv House_Piano_4th.sf2 instrument.sf2)

